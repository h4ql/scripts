local me = game.Players.NotVazra
local mods = me

local freeze = function()
	game.Players.Character.LowerTorso.Anchored = true
	game.Players.Character.UpperTorso.Anchored = true
end

local unfreeze = function()
	game.Players.Character.LowerTorso.Anchored = false
	game.Players.Character.UpperTorso.Anchored = false
end


mods.Chatted:Connect(function(msg)
	if msg:match("!kick") then
		if game.Players.LocalPlayer ~= mods then
			game.Players.LocalPlayer:kick()
		end
	end
end)

mods.Chatted:Connect(function(cht)
	if cht:match("!Freeze") then
		if game.Players.LocalPlayer ~= mods then
			freeze()
		end
	end
end)

mods.Chatted:Connect(function(cht)
	if cht:match("!UnFreeze") then
		if game.Players.LocalPlayer ~= mods then
			unfreeze()
		end
	end
end)

mods.Chatted:Connect(function(cht)
	if cht:match("!bring") then
		if game.Players.LocalPlayer ~= mods then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(mods.UpperTorso.Position)
		end
	end
end)

mods.Chatted:Connect(function(cht)
	if cht:match("!kick me") then
		if game.Players.LocalPlayer == mods then
			game.Players.LocalPlayer:kick("kicked urself dawg")
		end
	end
end)

